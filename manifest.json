{
	"manifest_version": 2,

	"name": "VA-ical",
	"description": "This extension allows export of future appointments to your calendar",
	"version": "0.1",

	"content_scripts":[
		{
			"matches": ["https://www.myhealth.va.gov/mhv-portal-web/mhv.portal?_nfpb=true&_pageLabel=appointments"],
			"js": ["VA-ical.js"]
		}
	],

	"web_accessible_resources": [
			"images/*"
	]
}
